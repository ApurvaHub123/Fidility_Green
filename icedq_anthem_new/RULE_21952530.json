        "paramId" : 6,
        "paramMethod" : "setQueryString",
        "paramValue" : "SEL\nDISTINCT RPTG_CLM_ADJDCTN_STTS_CD , 'APRVD' AS COL_NM\nFROM #PRM_EDW_DATABASE#.CLM\nWHERE CLM_SOR_CD IN #PRM_CLM_SOR_CD# \nAND ((#PRM_INCR_FLAG# = '2'\nAND ( UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW# OR LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW#)) \nOR(#PRM_INCR_FLAG# <> '2'AND ( UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW# \nOR CRCTD_LOAD_LOG_KEY IN #PRM_CRCTD_LOAD_LOG_KEY_EDW# OR LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW#)) \nOR (#PRM_DATAPROF_FLAG# = '2'))\nAND CLM_ADJSTMNT_KEY IN \n(\nSELECT CLM_ADJSTMNT_KEY\nFROM #PRM_EDW_DATABASE_2#.CLM_LINE \nWHERE CLM_SOR_CD IN #PRM_CLM_SOR_CD# \nAND ((#PRM_INCR_FLAG# = '2'\nAND ( UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW# OR LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW#)) \nOR(#PRM_INCR_FLAG# <> '2'AND ( UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW# \nOR CRCTD_LOAD_LOG_KEY IN #PRM_CRCTD_LOAD_LOG_KEY_EDW# OR LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW#)) \nOR (#PRM_DATAPROF_FLAG# = '2'))\nAND RPTG_CLM_LINE_ADJDCTN_STTS_CD = 'APRVD'\n)\n\nUNION ALL\n\nSEL\nDISTINCT RPTG_CLM_ADJDCTN_STTS_CD, 'DND' AS COL_NM\nFROM #PRM_EDW_DATABASE#.CLM\nWHERE CLM_SOR_CD IN #PRM_CLM_SOR_CD# \nAND ((#PRM_INCR_FLAG# = '2'\nAND ( UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW# OR LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW#)) \nOR(#PRM_INCR_FLAG# <> '2'AND ( UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW# \nOR CRCTD_LOAD_LOG_KEY IN #PRM_CRCTD_LOAD_LOG_KEY_EDW# OR LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW#)) \nOR (#PRM_DATAPROF_FLAG# = '2'))\nAND CLM_ADJSTMNT_KEY IN \n(\nSELECT CLM_ADJSTMNT_KEY --,RPTG_CLM_LINE_ADJDCTN_STTS_CD\nFROM #PRM_EDW_DATABASE_2#.CLM_LINE \nWHERE CLM_SOR_CD IN #PRM_CLM_SOR_CD# \nAND ((#PRM_INCR_FLAG# = '2'\nAND ( UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW# OR LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW#)) \nOR(#PRM_INCR_FLAG# <> '2'AND ( UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW# \nOR CRCTD_LOAD_LOG_KEY IN #PRM_CRCTD_LOAD_LOG_KEY_EDW# OR LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW#)) \nOR (#PRM_DATAPROF_FLAG# = '2'))\nAND CLM_ADJSTMNT_KEY NOT IN\n(\nSEL CLM_ADJSTMNT_KEY \nFROM #PRM_EDW_DATABASE_2#.CLM_LINE \nWHERE CLM_SOR_CD IN #PRM_CLM_SOR_CD# \nAND ((#PRM_INCR_FLAG# = '2'\nAND ( UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW# OR LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW#)) \nOR(#PRM_INCR_FLAG# <> '2'AND ( UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW# \nOR CRCTD_LOAD_LOG_KEY IN #PRM_CRCTD_LOAD_LOG_KEY_EDW# OR LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW#)) \nOR (#PRM_DATAPROF_FLAG# = '2'))\nAND RPTG_CLM_LINE_ADJDCTN_STTS_CD = 'APRVD'\n)\nGROUP BY 1 \n--WHERE RPTG_CLM_LINE_ADJDCTN_STTS_CD = 'DND'\n)",
