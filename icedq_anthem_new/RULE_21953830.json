        "paramId" : 6,
        "paramMethod" : "setQueryString",
        "paramValue" : "SEL \nCL.CLM_ADJSTMNT_KEY,CL.CLM_SOR_CD,CL.CLM_DIAG_ID,SRVC_RNDRG_TYPE_CD\n FROM #PRM_EDW_DATABASE#.CLM_LINE CL  \nLEFT OUTER JOIN #PRM_EDW_DATABASE_2#.CLM C ON CL.CLM_ADJSTMNT_KEY = C.CLM_ADJSTMNT_KEY\nWHERE \nCL.CLM_ADJSTMNT_KEY IS NOT NULL AND\nSRVC_RNDRG_TYPE_CD IN  ('HOSP','FANCL') AND CLM_DIAG_ID <>'00' AND CLM_DIAG_ID NOT IN('UNK','NA')\nAND CL.CLM_SOR_CD IN #PRM_CLM_SOR_CD#\nAND ((#PRM_INCR_FLAG# = '2'\nAND ( CL.UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW#  OR CL.LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW# ))\nOR(#PRM_INCR_FLAG# <> '2'AND ( CL.UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW#\nOR CL.CRCTD_LOAD_LOG_KEY IN #PRM_CRCTD_LOAD_LOG_KEY_EDW# OR CL.LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW# )))",
