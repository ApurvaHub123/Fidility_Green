      "paramsVOLst" : [ {
        "ruleClsPropID" : 21955384,
        "paramId" : 6,
        "paramMethod" : "setQueryString",
        "paramValue" : "SELECT \t\t\t\t\t\t\t\t\n'CLM_LINE'  AS TABLENAME,\t\t\t\t\t\t\t\t\nC.CLM_ADJSTMNT_KEY ,\t\t\t\t\t\t\t\t\nC.CLM_DISP_CD,\t\t\t\t\t\t\t\t\nSUM(CVRD_EXPNS_AMT) AS SUM_CVRD_EXPNS_AMT,\t\t\t\t\t\t\t\t\nSUM(BILLD_CHRG_AMT) AS SUM_BILLD_CHRG_AMT\t\t\t\t\t\t\t\t\nFROM #PRM_EDW_DATABASE_2#.CLM C\t\t\t\t\t\t\t\t\nINNER JOIN  #PRM_EDW_DATABASE#.CLM_LINE CL\t\t\t\t\t\t\t\t\n ON CL.CLM_ADJSTMNT_KEY=C.CLM_ADJSTMNT_KEY\t\t\t\t\t\t\t\t\n AND CL.CLM_SOR_CD = C.CLM_SOR_CD\t\t\t\t\t\t\t\t\nWHERE CLM_DISP_CD<>'RVRSL' \t\t\t\t\t\t\t\t\nand  CL.CLM_SOR_CD IN #PRM_CLM_SOR_CD#\t\t\t\t\t\t\t\t\nAND ((#PRM_INCR_FLAG# = '2'\t\t\t\t\t\t\t\t\nAND ( CL.UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW# OR  CL.LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW#)) \t\t\t\t\t\t\t\t\nOR(#PRM_INCR_FLAG# <> '2'AND (  CL.UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW# \t\t\t\t\t\t\t\t\nOR  CL.CRCTD_LOAD_LOG_KEY IN #PRM_CRCTD_LOAD_LOG_KEY_EDW# OR  CL.LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW#)) \t\t\t\t\t\t\t\t\nOR (#PRM_DATAPROF_FLAG# = '2'))\t\t\t\t\t\t\t\t\nGROUP BY 1,2,3\t\t\t\t\t\t\t\t\nHAVING  SUM_CVRD_EXPNS_AMT >SUM_BILLD_CHRG_AMT\t\t\t\t\t\t\t\t\n",
