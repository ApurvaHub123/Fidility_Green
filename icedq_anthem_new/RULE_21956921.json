      "paramsVOLst" : [ {
        "ruleClsPropID" : 21956933,
        "paramId" : 6,
        "paramMethod" : "setQueryString",
        "paramValue" : "SELECT CLM_SOR_CD, ADJDCTN_DT, COUNT (*) \nFROM \n( \nSELECT \nC.CLM_ADJSTMNT_KEY, C.CLM_SOR_CD, CLM_LINE_NBR, RPTG_CLM_ADJDCTN_STTS_CD,\nRPTG_CLM_LINE_ADJDCTN_STTS_CD, \nCASE WHEN CL.RPTG_CLM_LINE_ADJDCTN_STTS_CD IN ('APRVD','APRZB') THEN 1\nELSE 2\nEND AS CL_STTS_CD\n,C.ADJDCTN_DT \nFROM ( \nSEL CLM_ADJSTMNT_KEY,CLM_SOR_CD,RPTG_CLM_ADJDCTN_STTS_CD,ADJDCTN_DT \nFROM #PRM_EDW_DATABASE#.CLM\nWHERE CLM_SOR_CD IN #PRM_CLM_SOR_CD# \nAND ADJDCTN_DT BETWEEN #PRM_MIN_ADJDCTN_DT# AND #PRM_MAX_ADJDCTN_DT#   \nAND RPTG_CLM_ADJDCTN_STTS_CD IN('01','08' ) \nAND ((#PRM_INCR_FLAG# = '2'\nAND (UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW# OR LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW#)) \nOR(#PRM_INCR_FLAG# <> '2'AND ( UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW# \nOR CRCTD_LOAD_LOG_KEY IN #PRM_CRCTD_LOAD_LOG_KEY_EDW# OR LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW#)) \nOR (#PRM_DATAPROF_FLAG# = '2')) )C,  \n( \nSEL CLM_ADJSTMNT_KEY,CLM_LINE_NBR,CLM_SOR_CD,RPTG_CLM_LINE_ADJDCTN_STTS_CD,\nADJDCTN_DT\nFROM #PRM_EDW_DATABASE_2#.CLM_LINE \nWHERE CLM_SOR_CD IN #PRM_CLM_SOR_CD# \nAND ADJDCTN_DT BETWEEN #PRM_MIN_ADJDCTN_DT# AND #PRM_MAX_ADJDCTN_DT#\nAND ((#PRM_INCR_FLAG# = '2'\nAND (UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW# OR LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW#)) \nOR(#PRM_INCR_FLAG# <> '2'AND ( UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW# \nOR CRCTD_LOAD_LOG_KEY IN #PRM_CRCTD_LOAD_LOG_KEY_EDW# OR LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW#)) \nOR (#PRM_DATAPROF_FLAG# = '2'))\n)CL  \nWHERE C.CLM_ADJSTMNT_KEY=CL.CLM_ADJSTMNT_KEY \nQUALIFY MIN(CL_STTS_CD) OVER (PARTITION BY CL.CLM_ADJSTMNT_KEY) = 2\n) A \nGROUP BY 1,2",
