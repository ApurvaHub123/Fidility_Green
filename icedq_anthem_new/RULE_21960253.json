        "ruleClsPropID" : 21960270,
        "paramId" : 6,
        "paramMethod" : "setQueryString",
        "paramValue" : "SELECT \n'CLM_LINE'  AS TABLENAME,\nC.CLM_ADJSTMNT_KEY ,\nCLM_DISP_CD,\nSUM(CPAY_AMT) AS SUM_CPAY_AMT\nFROM\n#PRM_EDW_DATABASE_2#.CLM C  INNER JOIN #PRM_EDW_DATABASE#.CLM_LINE CL \nON CL.CLM_ADJSTMNT_KEY=C.CLM_ADJSTMNT_KEY\nAND CL.CLM_SOR_CD = C.CLM_SOR_CD\nWHERE CLM_DISP_CD<>'RVRSL' \n\nAND((#PRM_INCR_FLAG# = '2'\nAND (CL.UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW# OR CL.LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW#)) \nOR(#PRM_INCR_FLAG# <> '2'AND ( CL.UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW# \nOR CL.CRCTD_LOAD_LOG_KEY IN #PRM_CRCTD_LOAD_LOG_KEY_EDW# OR CL.LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW#)) \nOR (#PRM_DATAPROF_FLAG# = '2'))\n\nAND CL.CLM_SOR_CD IN #PRM_CLM_SOR_CD#\nGROUP BY 1,2,3\nHAVING  SUM_CPAY_AMT <0\n\nUNION ALL\n\nSELECT \n'CLM_LINE'  AS TABLENAME,\nC.CLM_ADJSTMNT_KEY ,\nCLM_DISP_CD,\nSUM(CPAY_AMT) AS SUM_CPAY_AMT\nFROM\n#PRM_EDW_DATABASE_2#.CLM C  INNER JOIN #PRM_EDW_DATABASE#.CLM_LINE CL\nON CL.CLM_ADJSTMNT_KEY=C.CLM_ADJSTMNT_KEY\nAND CL.CLM_SOR_CD = C.CLM_SOR_CD\nWHERE CLM_DISP_CD='RVRSL'\n \nAND((#PRM_INCR_FLAG# = '2'\nAND (CL.UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW# OR CL.LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW#)) \nOR(#PRM_INCR_FLAG# <> '2'AND ( CL.UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW# \nOR CL.CRCTD_LOAD_LOG_KEY IN #PRM_CRCTD_LOAD_LOG_KEY_EDW# OR CL.LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW#)) \nOR (#PRM_DATAPROF_FLAG# = '2'))\n\nAND CL.CLM_SOR_CD IN #PRM_CLM_SOR_CD#\nGROUP BY 1,2,3\nHAVING  SUM_CPAY_AMT >0",
