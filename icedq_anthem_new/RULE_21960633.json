      "paramsVOLst" : [ {
        "ruleClsPropID" : 21960645,
        "paramId" : 6,
        "paramMethod" : "setQueryString",
        "paramValue" : "SELECT CLM_ADJSTMNT_KEY ,\t\t\t\t\t\t\t\t\nSUM_BILLD_CHRG_AMT,\t\t\t\t\t\t\t\t\nSUM_PAID_AMT,\t\t\t\t\t\t\t\t\nSUM_CPAY_AMT\t\t\t\t\t\t\t\t\nFROM  (\t\t\t\t\t\t\t\t\nSELECT \t\t\t\t\t\t\t\t\nCLM_ADJSTMNT_KEY,\t\t\t\t\t\t\t\t\nABS(SUM(BILLD_CHRG_AMT)) AS SUM_BILLD_CHRG_AMT  ,\t\t\t\t\t\t\t\t\nABS(SUM(PAID_AMT)) AS  SUM_PAID_AMT,\t\t\t\t\t\t\t\t\nABS(SUM(CPAY_AMT)) AS SUM_CPAY_AMT \t\t\t\t\t\t\t\t\nFROM #PRM_EDW_DATABASE#.CLM_LINE CL\t\t\t\t\t\t\t\t\n \t\t\t\t\t\t\t\t\nGROUP BY 1\t\t\t\t\t\t\t\t\nWHERE CL.CLM_SOR_CD IN #PRM_CLM_SOR_CD#\t\t\t\t\t\t\t\t\nAND ((#PRM_INCR_FLAG# = '2'\t\t\t\t\t\t\t\t\nAND ( CL.UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW# OR  CL.LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW#)) \t\t\t\t\t\t\t\t\nOR(#PRM_INCR_FLAG# <> '2'AND (  CL.UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW# \t\t\t\t\t\t\t\t\nOR  CL.CRCTD_LOAD_LOG_KEY IN #PRM_CRCTD_LOAD_LOG_KEY_EDW# OR  CL.LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW#)) \t\t\t\t\t\t\t\t\nOR (#PRM_DATAPROF_FLAG# = '2'))\t) AS CLM \t\t\t\t\t\t\t\nWHERE \t\t\t\t\t\t\t\t\n SUM_BILLD_CHRG_AMT<SUM_PAID_AMT\t\t\t\t\t\t\t\t\n OR SUM_BILLD_CHRG_AMT <SUM_CPAY_AMT\t\t\t\t\t\t\t\t\n OR SUM_BILLD_CHRG_AMT <=0\t\t\t\t\t\t\t\t\n",
