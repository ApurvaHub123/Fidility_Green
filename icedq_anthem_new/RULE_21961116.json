        "paramId" : 6,
        "paramMethod" : "setQueryString",
        "paramValue" : "SEL CLM_ADJSTMNT_KEY, CLM_LINE_NBR,CLM_LINE_STTS_CD               \nFROM    \n(               \nSEL CL.CLM_ADJSTMNT_KEY, CLM_LINE_NBR,'APRVD' AS CLM_LINE_STTS_CD    \nFROM #PRM_EDW_DATABASE#.CLM_LINE CL           \nWHERE CLM_SOR_CD IN #PRM_CLM_SOR_CD# AND ((#PRM_INCR_FLAG# = '2'\nAND ( UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW#  OR LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW# ))\nOR(#PRM_INCR_FLAG# <> '2'AND ( UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW#\nOR CRCTD_LOAD_LOG_KEY IN #PRM_CRCTD_LOAD_LOG_KEY_EDW# OR LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW# )))      \nAND BILLD_CHRG_AMT = DSCNT_AMT AND BILLD_CHRG_AMT<>0          \nUNION ALL\n--2 (APRVD)           \nSEL CL.CLM_ADJSTMNT_KEY, CLM_LINE_NBR,'APRVD' AS CLM_LINE_STTS_CD    \nFROM #PRM_EDW_DATABASE#.CLM_LINE CL           \nWHERE CLM_SOR_CD IN #PRM_CLM_SOR_CD# AND ((#PRM_INCR_FLAG# = '2'\nAND ( UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW#  OR LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW# ))\nOR(#PRM_INCR_FLAG# <> '2'AND ( UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW#\nOR CRCTD_LOAD_LOG_KEY IN #PRM_CRCTD_LOAD_LOG_KEY_EDW# OR LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW# )))    \n AND BILLD_CHRG_AMT = DSCNT_AMT\nAND (DSCNT_AMT <> 0 OR CVRD_EXPNS_AMT <> 0 OR DDCTBL_AMT<>0 OR COINSRN_AMT<>0 OR CPAY_AMT<>0   \nOR MBR_SNCTNS_PNLTYS_AMT <>0 OR COB_SVNGS_AMT<>0 OR PAT_PAID_DFRNTL_AMT<>0      \nOR ENRLMNT_PRTCTN_AMT<>0 OR EPO_WTHLD_AMT<>0 OR FFS_EQVLNT_AMT<>0 OR PAID_AMT<>0)      \nUNION ALL  \n--3 (APRVD)           /*\nSEL CL.CLM_ADJSTMNT_KEY, CLM_LINE_NBR,'APRVD' AS CLM_LINE_STTS_CD    \nFROM #PRM_EDW_DATABASE#.CLM_LINE CL           \nWHERE CLM_SOR_CD IN #PRM_CLM_SOR_CD# AND ((#PRM_INCR_FLAG# = '2'\nAND ( UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW#  OR LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW# ))\nOR(#PRM_INCR_FLAG# <> '2'AND ( UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW#\nOR CRCTD_LOAD_LOG_KEY IN #PRM_CRCTD_LOAD_LOG_KEY_EDW# OR LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW# )))     \nAND --BILLD_CHRG_AMT = 0  OR  \n\n   BILLD_CHRG_AMT <> (NON_CVRD_AMT +    DSCNT_AMT ) AND\n(NON_CVRD_AMT<>0 OR CVRD_EXPNS_AMT<>0  OR ALWD_AMT<>0 OR OTHR_RDCTN_AMT<>0     \nOR DSCNT_AMT<>0 OR DDCTBL_AMT<>0 OR COINSRN_AMT<>0 OR CPAY_AMT<>0           \nOR MBR_SNCTNS_PNLTYS_AMT <>0 OR COB_SVNGS_AMT<>0 OR PAT_PAID_DFRNTL_AMT<>0      \nOR ENRLMNT_PRTCTN_AMT<>0 OR EPO_WTHLD_AMT<>0 OR FFS_EQVLNT_AMT<>0 OR PAID_AMT<>0)   \n        \n)INN          \nMINUS   \nSEL CL.CLM_ADJSTMNT_KEY, CLM_LINE_NBR,CLM_LINE_STTS_CD         \nFROM #PRM_EDW_DATABASE#.CLM_LINE CL           \nWHERE CLM_SOR_CD IN #PRM_CLM_SOR_CD# AND ((#PRM_INCR_FLAG# = '2'\nAND ( UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW#  OR LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW# ))\nOR(#PRM_INCR_FLAG# <> '2'AND ( UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW#\nOR CRCTD_LOAD_LOG_KEY IN #PRM_CRCTD_LOAD_LOG_KEY_EDW# OR LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW# )))   \nAND CLM_LINE_STTS_CD IN ('APRVD') ",
