      "paramsVOLst" : [ {
        "ruleClsPropID" : 21961217,
        "paramId" : 6,
        "paramMethod" : "setQueryString",
        "paramValue" : "SEL 'NON_CVRD_AMT' AS AMT_NM, SUM(NON_CVRD_AMT) AS AMT_SUM,SUM(RDCTN_AMT) AS SUM_RDCTN_AMT\nFROM #PRM_EDW_DATABASE#.CLM_LINE CL\nJOIN #PRM_EDW_DATABASE_2#.CLM_LINE_RDCTN CLR\nON CL.CLM_ADJSTMNT_KEY = CLR.CLM_ADJSTMNT_KEY\nAND CL.CLM_LINE_NBR = CLR.CLM_LINE_NBR\nAND CL.LOAD_LOG_KEY = CLR.LOAD_LOG_KEY\nWHERE CL.CLM_SOR_CD IN #PRM_CLM_SOR_CD# \nAND CLR.RDCTN_TYPE_CD IN ('3','5')\nAND ((#PRM_INCR_FLAG# = '2'\nAND ( CL.UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW#  OR CL.LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW# ))\nOR(#PRM_INCR_FLAG# <> '2'AND ( CL.UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW#\nOR CL.CRCTD_LOAD_LOG_KEY IN #PRM_CRCTD_LOAD_LOG_KEY_EDW# OR CL.LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW# )))\n\nUNION ALL\n\nSEL 'DSCNT_AMT' AS AMT_NM, SUM(DSCNT_AMT) AS AMT_SUM,SUM(RDCTN_AMT) AS SUM_RDCTN_AMT\nFROM #PRM_EDW_DATABASE#.CLM_LINE CL\nJOIN #PRM_EDW_DATABASE_2#.CLM_LINE_RDCTN CLR\nON CL.CLM_ADJSTMNT_KEY = CLR.CLM_ADJSTMNT_KEY\nAND CL.CLM_LINE_NBR = CLR.CLM_LINE_NBR\nAND CL.LOAD_LOG_KEY = CLR.LOAD_LOG_KEY\nWHERE CL.CLM_SOR_CD IN #PRM_CLM_SOR_CD# \nAND CLR.RDCTN_TYPE_CD IN ('2')\nAND ((#PRM_INCR_FLAG# = '2'\nAND ( CL.UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW#  OR CL.LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW# ))\nOR(#PRM_INCR_FLAG# <> '2'AND ( CL.UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW#\nOR CL.CRCTD_LOAD_LOG_KEY IN #PRM_CRCTD_LOAD_LOG_KEY_EDW# OR CL.LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW# )))\n\nUNION ALL\n\nSEL 'OTHR_RDCTN_AMT' AS AMT_NM, SUM(OTHR_RDCTN_AMT) AS AMT_SUM,SUM(RDCTN_AMT) AS SUM_RDCTN_AMT\nFROM #PRM_EDW_DATABASE#.CLM_LINE CL\nJOIN #PRM_EDW_DATABASE_2#.CLM_LINE_RDCTN CLR\nON CL.CLM_ADJSTMNT_KEY = CLR.CLM_ADJSTMNT_KEY\nAND CL.CLM_LINE_NBR = CLR.CLM_LINE_NBR\nAND CL.LOAD_LOG_KEY = CLR.LOAD_LOG_KEY\nWHERE CL.CLM_SOR_CD IN #PRM_CLM_SOR_CD# \nAND CLR.RDCTN_CTGRY_CD IN ('CO', 'RI')\nAND ((#PRM_INCR_FLAG# = '2'\nAND ( CL.UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW#  OR CL.LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW# ))\nOR(#PRM_INCR_FLAG# <> '2'AND ( CL.UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW#\nOR CL.CRCTD_LOAD_LOG_KEY IN #PRM_CRCTD_LOAD_LOG_KEY_EDW# OR CL.LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW# )))\n\nUNION ALL\n\nSEL 'COINSRN_AMT' AS AMT_NM, SUM(COINSRN_AMT) AS AMT_SUM,SUM(RDCTN_AMT) AS SUM_RDCTN_AMT\nFROM #PRM_EDW_DATABASE#.CLM_LINE CL\nJOIN #PRM_EDW_DATABASE_2#.CLM_LINE_RDCTN CLR\nON CL.CLM_ADJSTMNT_KEY = CLR.CLM_ADJSTMNT_KEY\nAND CL.CLM_LINE_NBR = CLR.CLM_LINE_NBR\nAND CL.LOAD_LOG_KEY = CLR.LOAD_LOG_KEY\nWHERE CL.CLM_SOR_CD IN #PRM_CLM_SOR_CD# \nAND  CLR.RDCTN_CTGRY_CD IN ('CI')\n\nUNION ALL\n\nSEL 'COB_SVNGS_AMT' AS AMT_NM, SUM(COB_SVNGS_AMT) AS AMT_SUM,SUM(RDCTN_AMT) AS SUM_RDCTN_AMT\nFROM #PRM_EDW_DATABASE#.CLM_LINE CL\nJOIN #PRM_EDW_DATABASE_2#.CLM_LINE_RDCTN CLR\nON CL.CLM_ADJSTMNT_KEY = CLR.CLM_ADJSTMNT_KEY\nAND CL.CLM_LINE_NBR = CLR.CLM_LINE_NBR\nWHERE CL.CLM_SOR_CD IN #PRM_CLM_SOR_CD#\nAND CLR.RDCTN_CTGRY_CD IN ('CS')\nAND ((#PRM_INCR_FLAG# = '2'\nAND ( CL.UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW#  OR CL.LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW# ))\nOR(#PRM_INCR_FLAG# <> '2'AND ( CL.UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW#\nOR CL.CRCTD_LOAD_LOG_KEY IN #PRM_CRCTD_LOAD_LOG_KEY_EDW# OR CL.LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW# )))\n\nUNION ALL\n\nSEL 'DDCTBL_AMT' AS AMT_NM, SUM(DDCTBL_AMT) AS AMT_SUM,SUM(RDCTN_AMT) AS SUM_RDCTN_AMT\nFROM #PRM_EDW_DATABASE#.CLM_LINE CL\nJOIN #PRM_EDW_DATABASE_2#.CLM_LINE_RDCTN CLR\nON CL.CLM_ADJSTMNT_KEY = CLR.CLM_ADJSTMNT_KEY\nAND CL.CLM_LINE_NBR = CLR.CLM_LINE_NBR\nWHERE CL.CLM_SOR_CD IN #PRM_CLM_SOR_CD#\nAND CLR.RDCTN_CTGRY_CD IN ('D1')\nAND ((#PRM_INCR_FLAG# = '2'\nAND ( CL.UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW#  OR CL.LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW# ))\nOR(#PRM_INCR_FLAG# <> '2'AND ( CL.UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW#\nOR CL.CRCTD_LOAD_LOG_KEY IN #PRM_CRCTD_LOAD_LOG_KEY_EDW# OR CL.LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW# )))",
