      "paramsVOLst" : [ {
        "ruleClsPropID" : 21961277,
        "paramId" : 6,
        "paramMethod" : "setQueryString",
        "paramValue" : "SEL CL.CLM_ADJSTMNT_KEY\n ,CL.CLM_LINE_NBR\n ,CL.MEDCR_OC_SQSTRTN_AMT\n FROM #PRM_EDW_DATABASE#.CLM_LINE CL\nINNER JOIN  #PRM_EDW_DATABASE_2#.CLM  C\nON CL.CLM_ADJSTMNT_KEY=C.CLM_ADJSTMNT_KEY\nAND C.SRVC_RNDRG_TYPE_CD IN (/* 'PHYSN', 'PANCL',*/ 'HOSP', 'FANCL' )\nAND ((#PRM_INCR_FLAG# = '2'\nAND ( CL.UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW# OR CL.LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW#)) \nOR(#PRM_INCR_FLAG# <> '2'AND ( CL.UPDTD_LOAD_LOG_KEY IN #PRM_UPDTD_LOAD_LOG_KEY_EDW# \nOR CL.CRCTD_LOAD_LOG_KEY IN #PRM_CRCTD_LOAD_LOG_KEY_EDW# OR CL.LOAD_LOG_KEY IN #PRM_LOAD_LOG_KEY_EDW#)) \nOR (#PRM_DATAPROF_FLAG# = '2'))\n\nMINUS\n\nSELECT\n\nCASE WHEN CLMP.DDC_CD_CURR_STS_CDE IN ('88','99')\n         THEN MD5('$LZ2CSA_CLM_SOR_CD'||'_'||TRIM(CLMP.DDC_EXT_DCN_CC)\n          ||'_'||TRIM(CLMP.DDC_EXT_DCN_NO)\n          ||'_'||TRIM(CAST(CLMP.DDC_EXT_SEQ_NUMBER  AS CHAR(3)))\n          ||'_'||TRIM(CLMP.DDC_EXT_DCN_ITM_CDE)\n          ||'_'||TRIM(CLMP1.DER_DDC_CD_CLM_COMPL_DTE))  \n         ELSE ' '\n    END AS CLM_ADJSTMNT_KEY1 \n,CASE    WHEN DTLP.DDC_DTL_LNE_NBR IS NULL OR DTLP.DDC_DTL_LNE_NBR<=0 THEN 'UNK'\n                   WHEN DTLP.DDC_DTL_LNE_NBR>0 AND DTLP.DDC_DTL_LNE_NBR<10 THEN  '0'||CAST(CAST(DTLP.DDC_DTL_LNE_NBR AS BIGINT) \n                   AS VARCHAR(6))\n                   ELSE CAST(CAST(DTLP.DDC_DTL_LNE_NBR AS BIGINT) AS VARCHAR(6)) \n           END AS CLM_LINE_NBR\n     ,DDC_MED_PART_B_SEQ_AMT\n      FROM    \n    LZ_WGS_GNCCLMP CLMP\n    INNER JOIN \n         (SELECT \n     CASE \n        WHEN DER1.DDC_CD_CLM_COMPL_DTE>16000000 AND DER1.DDC_CD_CLM_COMPL_DTE<100000000\n                THEN CAST(DER1.DDC_CD_CLM_COMPL_DTE AS VARCHAR(8))\n               ELSE CAST('88881231' AS  VARCHAR(8))\n        END  AS DER_DDC_CD_CLM_COMPL_DTE\n  \n   ,CASE WHEN DER1.DDC_CD_FULL_MBU_CDE IS NULL THEN ' ' ELSE DER1.DDC_CD_FULL_MBU_CDE END AS DDC_CD_FULL_MBU_CDE_DER\n   ,CASE WHEN DER1.DDC_CD_SLC_CDE IS NULL THEN ' ' ELSE DER1.DDC_CD_SLC_CDE END AS DDC_CD_SLC_CDE_DER\n   ,CASE WHEN DER1.DDC_CD_MK_FUND_TYPE IS NULL THEN ' ' ELSE DER1.DDC_CD_MK_FUND_TYPE END AS DDC_CD_MK_FUND_TYPE_DER\n   ,CASE WHEN DER1.DDC_CD_PRIM_COV_CDE IS NULL THEN ' ' ELSE DER1.DDC_CD_PRIM_COV_CDE END AS DDC_CD_PRIM_COV_CDE_DER\n   ,CASE WHEN DER1.DDC_CD_PRIM_NET_CDE IS NULL THEN ' ' ELSE DER1.DDC_CD_PRIM_NET_CDE END AS DDC_CD_PRIM_NET_CDE_DER\n                 ,SUBSTR(TRIM(DDC_CD_FULL_MBU_CDE_DER),1,3)||TRIM(DDC_CD_SLC_CDE_DER)||TRIM(DDC_CD_MK_FUND_TYPE_DER)||\n    TRIM(DDC_CD_PRIM_COV_CDE_DER)||TRIM(DDC_CD_PRIM_NET_CDE_DER)||'X' AS OLD_LOB\n                 ,DER1.DDC_EXT_DCN_CC\n   ,DER1.DDC_EXT_DCN_NO\n   ,DER1.DDC_EXT_DCN_ITM_CDE\n   ,DER1.DDC_EXT_SEQ_NUMBER\n   ,DER1.DDC_CD_CLM_COMPL_DTE\n   ,DER1.LOAD_LOG_KEY\n   ,CAST(SUBSTR(DER1.DDC_EXT_DCN_NO,3,5) AS CHAR(3)) AS DAY_OF_YEAR\n                 ,DER1.DDC_EXT_DCN_CC|| SUBSTR(DER1.DDC_EXT_DCN_NO,1,2) AS YEAR_OF_CALENDAR\n\n          FROM LZ_WGS_GNCCLMP DER1) CLMP1\n   \n\n         ON   CLMP.DDC_EXT_DCN_CC       = CLMP1.DDC_EXT_DCN_CC \n AND   CLMP.DDC_EXT_DCN_NO       = CLMP1.DDC_EXT_DCN_NO \n AND   CLMP.DDC_EXT_DCN_ITM_CDE  = CLMP1.DDC_EXT_DCN_ITM_CDE\n AND   CLMP.DDC_EXT_SEQ_NUMBER   = CLMP1.DDC_EXT_SEQ_NUMBER\n AND   CLMP.DDC_CD_CLM_COMPL_DTE = CLMP1.DDC_CD_CLM_COMPL_DTE\n AND   CLMP.LOAD_LOG_KEY         = CLMP1.LOAD_LOG_KEY \n INNER JOIN LZ_WGS_GNCDTLP DTLP\n  \n                  ON CLMP.DDC_EXT_DCN_CC = DTLP.DDC_EXT_DCN_CC \n                 AND CLMP.DDC_EXT_DCN_NO = DTLP.DDC_EXT_DCN_NO \n                 AND CLMP.DDC_EXT_DCN_ITM_CDE= DTLP.DDC_EXT_DCN_ITM_CDE\n                 AND CLMP.DDC_EXT_SEQ_NUMBER = DTLP.DDC_EXT_SEQ_NUMBER                 \n                 AND CLMP.DDC_CD_CLM_COMPL_DTE=DTLP.DDC_CD_CLM_COMPL_DTE\n     AND CLMP.LOAD_LOG_KEY        = DTLP.LOAD_LOG_KEY \n \n LEFT OUTER JOIN LZ_WGS_GNCMEDP MEDP\nON DTLP.DDC_EXT_DCN_CC=MEDP.DDC_EXT_DCN_CC \nAND    DTLP.DDC_EXT_DCN_NO=MEDP.DDC_EXT_DCN_NO \nAND    DTLP.DDC_EXT_DCN_ITM_CDE=MEDP.DDC_EXT_DCN_ITM_CDE\nAND    DTLP.DDC_EXT_SEQ_NUMBER=MEDP.DDC_EXT_SEQ_NUMBER\nAND    DTLP.DDC_CD_CLM_COMPL_DTE=MEDP.DDC_CD_CLM_COMPL_DTE",
